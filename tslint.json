{
	"rules": {
		"no-string-throw": true,
		"no-unused-expression": true,
		"no-duplicate-variable": true,
		"curly": true,
		"class-name": true,
		"semicolon": [
			true,
			"always"
		],
		"no-default-export": true,
		"interface-name": true,
		"triple-equals": {
            "options": [
                "allow-null-check",
                "allow-undefined-check"
            ]
        }
	},
	"defaultSeverity": "warning"
}
